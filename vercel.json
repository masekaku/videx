{
  "routes": [
    {
      "src": "^/v/(.*)$",
      "dest": "/index.html"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "Cache-Control": "no-store" },
      "dest": "/assets/$1"
    },
    {
      "src": "/404.html",
      "dest": "/404.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}