{
  "version": 2,
  "routes": [
    {
      "src": "^/v/(.*)$",
      "dest": "/v/index.html"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "no-store"
      },
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}